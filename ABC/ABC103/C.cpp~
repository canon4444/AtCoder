#include <iostream>
using namespace std;

int main()
{
    int N;
    cin >> N;

    int a[3000] = { 0 };
    for( int i = 0; i < N; ++i )
        cin >> a[i];

    long int m = 0;
    for( int i = 0; i < N; ++i )
        m += a[i];

    long int f, ans = 0;
    for( int i = m; -1 < i; --i ){
        f = 0;
        for( int j = 0; j < N; ++j ){
            f += i % a[j];
        }
        cout << i << " : " << f << endl;
        if( ans < f ) ans = f;
    }

    cout << ans << endl;

    return 0;
}
